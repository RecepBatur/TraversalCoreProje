@model IEnumerable<BookingExchangeSecondViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Booking Api Kur Bilgileri</h2>
<br />
<table class="table table-striped">

    <tr>
        <th>Kur Adı</th>
        <th>Kur Oranı</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.currency</th>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }

</table>
